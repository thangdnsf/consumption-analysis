{
  "manifest_version": 2,
  "name": "Consumsis",
  "description": "Consumption analysis",
  "author": "Thang Nguyen and Pratvi",
  "version": "2.0",
  "icons": {
    "128": "icon/icon128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "<all_urls>",
    "background"
  ],
  "background": {
    "scripts": ["jquery-3.4.1.min.js","background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon/icon128.png"
  },
  "content_scripts":[{
    "matches" : ["https://*/*","https://*/*"],
    "js":["script.js"]
  }]
}
