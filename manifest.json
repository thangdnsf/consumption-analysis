{
  "manifest_version": 2,
  "name": "Project",
  "description": "",
  "author": "",
  "version": "1.0",
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["jquery-3.4.1.min.js","background.js"]
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts":[{
    "matches" : ["https://*/*","https://*/*"],
    "js":["script.js"]
  }]
}
